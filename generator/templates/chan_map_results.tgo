var Err{{$name}}ChanTo{{.Name}} = errors.New("cannot convert {{$name}}ChanMapResult to chan {{.Type}}")

func (r {{$name}}ChanMapResult) To{{.Name}}() (chan {{.Type}}, chan error) {
        out := make(chan {{.Type}})
        err := make(chan error)

        finish := func() {
		close(err)
		close(out)
	}

	go func() {
		for v := range r {
			if _, ok := v.({{.Type}}); !ok {
				err <- Err{{$name}}ChanTo{{.Name}}
				finish()
